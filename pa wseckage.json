{
  "name": "estate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --prefix-colors cyan,magenta,yellow --prefix \"[{name}]\" \"vite --port 3009\" \"npm run start-node\" \"npm run start-python\"",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start-node": "cd node-server && npm start",
